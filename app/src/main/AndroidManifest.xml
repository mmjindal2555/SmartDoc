<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silk.smartdoc">

    <application
        android:name=".Controller.SmartDocManager"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".View.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".View.SignUp"
            android:label="@string/title_activity_sign_up"></activity>
        <activity
            android:name=".View.TestSearchActivity"
            android:label="@string/title_activity_test_search"></activity>
        <activity
            android:name=".View.TestSearchResultsActivity"
            android:label="@string/title_activity_test_search_results"></activity>
        <activity
            android:name=".View.AnswerQuery"
            android:label="@string/title_activity_answer_query"></activity>
        <activity
            android:name=".View.MedicineResult"
            android:label="@string/title_activity_medicine_result"></activity>
        <activity
            android:theme= "@style/AppTheme2"
            android:name=".View.MedicineSearch"
            android:label="@string/title_activity_medicine_search">
        </activity>
        <activity
            android:name=".View.UserAnswerQuery"
            android:label="@string/title_activity_user_answer_query"></activity>
        <activity
            android:name=".View.HealthForum"
            android:label="@string/title_activity_health_forum">

        </activity>
        <activity
            android:name=".View.PostQueryExperience"
            android:label="@string/title_activity_post_query_experience"></activity>

    </application>

</manifest>